{
  "appId": "org.bluescope.dcs",
  "appName": "Bluescope DCS",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com",
        "phone"
      ],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": [
          "profile",
          "https://www.googleapis.com/auth/drive"
        ]
      }
    }
  }
}